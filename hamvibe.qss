QWidget#freqSectionWidget {
    background-color: #ffaa00;
}

QPushButton#onAirLabel[onAir="true"] {
    color: white;
    background-color: red;
}

QPushButton#onAirLabel[onAir="false"] {
    color: white;
    background-color: black;
}

QPushButton[selectedBand="true"] {
    border: 2px solid #1e88e5;
}
